  <DataSets>
    <DataSet Name="setEjercicio03">
      <Query>
        <DataSourceName>dsEjercicio03</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@subcategoria_id">
            <Value>=Parameters!subcategoria_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  categorias.nombre AS [categorias nombre]
  ,productos.nombre AS [productos nombre]
  ,productos.precio
  ,productos.stock
FROM
  categorias
  INNER JOIN productos
    ON categorias.id = productos.categoria_id
WHERE
  productos.categoria_id = @subcategoria_id</CommandText>
